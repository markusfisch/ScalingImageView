<FindBugsFilter>
	<Match>
		<Class name="~.*R\$.*"/>
	</Match>
	<Match>
		<Class name="~.*Manifest\$.*"/>
	</Match>
	<Match>
		<Class name="~.android.support.*"/>
	</Match>
	<!-- this fall through is intentional and accordingly commented -->
	<Match>
		<Class name="de.markusfisch.android.scalingimageview.widget.ScalingImageView"/>
		<Method name="onTouchEvent"/>
		<Bug pattern="SF_SWITCH_FALLTHROUGH"/>
	</Match>
</FindBugsFilter>
